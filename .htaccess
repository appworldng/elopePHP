# RewriteRule ^category/([a-zA-Z0-9-]+)$ category.php?controller=category&parameter=$1
# RewriteRule ^(.*)/images/(.*)$ images/$2 [QSA,L]
# RewriteRule ^(.*)/css/(.*)$ css/$2 [QSA,L]
# RewriteRule ^(.*)/js/(.*)$ js/$2 [QSA,L]

